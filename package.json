{
  "name": "coffass",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "./src/lib/database/schema.prisma"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css}' && next lint --fix && prisma format",
    "test": "jest --testMatch='**/*.test.ts'",
    "test:small": "jest --testMatch='**/*.s.test.ts'",
    "test:medium": "jest --testMatch='**/*.m.test.ts'",
    "test:large": "jest --testMatch='**/*.l.test.ts'",
    "prisma": "prisma",
    "migration:validate": "prisma validate",
    "migration:generate": "prisma generate",
    "migration:reset": "prisma migrate reset",
    "migration:dev": "prisma migrate dev",
    "migration:push": "prisma db push",
    "migration:deploy": "prisma generate && prisma migrate deploy",
    "migration:resolve": "prisma migrate resolve",
    "migration:status": "prisma migrate status",
    "migration:diff": "prisma migrate diff"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.7",
    "@aws-sdk/client-s3": "^3.477.0",
    "@aws-sdk/lib-storage": "^3.477.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.5.2",
    "@react-email/components": "0.0.11",
    "deepmerge": "^4.3.1",
    "flowbite": "^2.0.0",
    "flowbite-react": "^0.7.0",
    "next": "14.0.1",
    "next-auth": "^5.0.0-beta.3",
    "react": "^18",
    "react-avatar-editor": "^13.0.1",
    "react-dom": "^18",
    "react-email": "1.9.5",
    "react-icons": "^4.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.0"
  },
  "devDependencies": {
    "@quramy/jest-prisma": "^1.7.3",
    "@quramy/prisma-fabbrica": "^2.0.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-avatar-editor": "^13.0.2",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemailer": "^6.9.7",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "typesafe-utils": "^1.16.2",
    "typescript": "^5"
  }
}
